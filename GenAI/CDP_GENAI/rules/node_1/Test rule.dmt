        <Preconditions>
            <Text><![CDATA[definitions 
	set 'variable2' to  a subject in the subjects of 'Carepathway' 
	where the clientId of this subject is not one of { "439658" , "05539" , "123456" } ;]]></Text>
        </Preconditions>
        <Structure>
            <ConditionDefinitions>
                <ConditionDefinition Id="C0">
                    <ExpressionDefinition>
                        <Text><![CDATA[the clientId of 'variable2' is not null is <a boolean>]]></Text>
                    </ExpressionDefinition>
                </ConditionDefinition>
            </ConditionDefinitions>
            <ActionDefinitions>
                <ActionDefinition Id="A0">
                    <ExpressionDefinition>
                        <Text><![CDATA[add a new Response where
	the ClientID is the clientId of 'variable2' , 
	generateDescriptiveCarePathway is <generateDescriptiveCarePathway> , 
	the Message is <Message> to 'decision']]></Text>
                    </ExpressionDefinition>
                </ActionDefinition>
